@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Pick Cargo</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="@Url.Content("~/Css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Css/font-awesome-4.7.0/css/font-awesome.min.css")" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css" async>
    <link href="@Url.Content("~/Css/AdminLTE.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Css/_all-skins.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/jquery-2.2.3.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datepicker.js")"></script>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" />

    <style type="text/css">
        input[type=text].error, input[type=password].error, textarea.error, select.error {
            border: 1px solid red;
        }

        label.error {
            display: none !important;
        }

        .hand {
            cursor: pointer;
        }

        .bootstrap-datetimepicker-widget {
            border-color: #d2d6de;
        }
    </style>
    @RenderSection("seccss", false)
</head>
<body class="skin-blue sidebar-mini sa wysihtml5-supported">
    <div class="wrapper">
        @{ Html.RenderPartial("_Header"); }
        @{ Html.RenderPartial("_Sidebar"); }
        <div class="content-wrapper">
            @RenderBody()
        </div>
        @{ Html.RenderPartial("_Footer"); }
    </div>

    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.slimscroll.min.js")"></script>
    <script src="@Url.Content("~/Scripts/fastclick.js")"></script>
    <script src="@Url.Content("~/Scripts/app.min.js")"></script>
    <script src="@Url.Content("~/Scripts/demo.js")"></script>

    <script type="text/javascript">
        var UrlAddress = '@Url.Content("~/Address")';
    </script>
    @RenderSection("secscripts", false)
    <!-- netstat -n -->
</body>
</html>
