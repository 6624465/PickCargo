<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/bootstrap.min.css")">
    <!-- Template styles-->
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/style.css")">
    <!-- Responsive styles-->
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/responsive.css")">
    <!-- FontAwesome -->
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/font-awesome.min.css")">
    <!-- Animation -->
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/animate.css")">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/owl.carousel.css")">
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/owl.theme.css")">
    <!-- Colorbox -->
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/colorbox.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Css/help/css/ripple.css")">

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/bootstrap-select.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Css/help/css/extra.css")">
    <style type="text/css">
        .btn-link {
            color: black;
        }

        .dt-title span {
            font-weight: bold;
            font-size: 26px;
        }
    </style>
</head>
<body>
    <div>
        @RenderBody()
    </div>
    <script src="@Url.Content("~/Css/help/js/jquery.js")"></script>


    <!-- Bootstrap jQuery -->
    <script type="text/javascript" src="@Url.Content("~/Css/help/js/bootstrap.min.js")"></script>
    <!-- Owl Carousel -->
    <script type="text/javascript" src="@Url.Content("~/Css/help/js/owl.carousel.min.js")"></script>
    <!-- Counter -->
    <script type="text/javascript" src="@Url.Content("~/Css/help/js/jquery.counterup.min.js")"></script>
    <!-- Waypoints -->
    <!-- Latest compiled and minified JavaScript -->
    <script src="@Url.Content("~/Css/help/js/bootstrap-select.min.js")"></script>


    <script type="text/javascript" src="@Url.Content("~/Css/help/js/waypoints.min.js")"></script>
    <!-- Color box -->
    <script type="text/javascript" src="@Url.Content("~/Css/help/js/jquery.colorbox.js")"></script>
    <!-- Isotope -->
    <script type="text/javascript" src="@Url.Content("~/Css/help/js/isotope.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Css/help/js/ini.isotope.js")"></script>


    <script type="text/javascript" src="@Url.Content("~/Css/help/js/owl.carousel.js")"></script>


    <script type="text/javascript" src="@Url.Content("~/Css/help/js/ripple.js")"></script>
    <script type="text/javascript">
        $(function () {
            $('.ripple').materialripple();
            $('.left-menu li a,.btn').materialripple();
            $('[data-toggle="tooltip"]').tooltip();

            $('.left-menu li.active').removeClass('active');
            //$('.dt-title span').hide();
        });

    </script>
</body>
</html>
